#include <bits/stdc++.h>

using namespace std;

int main() 
 {
 int n, k, a[100], c=0, i, j;
 cin>>n;
 cin>>k;
 for(i=0; i<n; i++) 
        {
                cin>>a[i];
        }
 for(i=0;i<n;i++)
        {
         for(j=n-1;j>=0; j--)
                {
                if(i<j) 
                        {
                         if((a[i]+a[j])%k==0)
                                {
                                 c++;
                                }
                        }
                }
        }
 cout<<c;
}





      
