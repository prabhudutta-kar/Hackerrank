#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int n; double a=0, b=0, c=0; 
    scanf("%d",&n);
    int arr[n];
    for(int arr_i = 0; arr_i < n; arr_i++){
       scanf("%d",&arr[arr_i]);
    }
    for(int arr_i = 0; arr_i < n; arr_i++){
        if(arr[arr_i] > 0)
            a++;
   else if(arr[arr_i] < 0)
            b++;
    else
        c++;
    }
    double p,q,r;
    printf("%f",p = a/n);
    printf("\n%f",q = b/n);
    printf("\n%f",r = c/n); 
    return 0;
}
