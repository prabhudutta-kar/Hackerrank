#include <bits/stdc++.h>

using namespace std;

int main()
{
 int n,i,c=0,d=0;
 int s[10000];
 cin>>n;
 for(i=0;i<n;i++)
        {
         cin>>s[i];
        }
 int m=s[0];
 for(i=1;i<n;i++)
        {
         if(s[i]>m)
                {
                 m=s[i];
                 c++;
                }
        }
 int p=s[0];
 for(i=0;i<n;i++)
        {
         if(s[i]<p)
                {
                 p=s[i];
                 d++;
                }
        }
 cout<<c<<" "<<d;
 return 0;
}

                  
